<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root minHeight="-Infinity" minWidth="-Infinity" prefHeight="398.0" prefWidth="107.0" styleClass="Panes"
         stylesheets="@stylesheets/application.css" type="VBox" xmlns="http://javafx.com/javafx/11.0.1"
         xmlns:fx="http://javafx.com/fxml/1">
	<Button fx:id="searchBtn" alignment="BASELINE_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
	        onAction="#searchButtonPressed" prefHeight="63.0" styleClass="menu-button" text="Search">
		<graphic>
			<ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
				<Image url="@../icons/Search.png"/>
			</ImageView>
		</graphic>
	</Button>
	<Button fx:id="myListBtn" alignment="BASELINE_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
	        onAction="#myListButtonPressed" prefHeight="63.0" styleClass="menu-button" text="MyList">
		<graphic>
			<ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
				<Image url="@../icons/MyList.png"/>
			</ImageView>
		</graphic>
	</Button>
	<Button fx:id="toWatchBtn" alignment="BASELINE_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
	        onAction="#toWatchButtonPressed" prefHeight="63.0" styleClass="menu-button" text="ToWatch">
		<graphic>
			<ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
				<Image url="@../icons/ToWatch.png"/>
			</ImageView>
		</graphic>
	</Button>
	<Button fx:id="customBtn" alignment="BASELINE_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
	        onAction="#customButtonPressed" prefHeight="63.0" styleClass="menu-button" text="Custom">
		<graphic>
			<ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
				<Image url="@../icons/Custom.png"/>
			</ImageView>
		</graphic>
	</Button>
	<HBox alignment="CENTER" prefHeight="46.0" prefWidth="77.0" spacing="15.0">
		<VBox.margin>
			<Insets top="30.0"/>
		</VBox.margin>
		<Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
		        mnemonicParsing="false" onAction="#openHomeScreen" prefHeight="32.0" prefWidth="40.0"
		        styleClass="misc-menu-button">
			<graphic>
				<ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
					<Image url="@../icons/Home.png"/>
				</ImageView>
			</graphic>
		</Button>
		<Button fx:id="settingsBtn" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
		        minWidth="-Infinity" mnemonicParsing="false" onAction="#openSettingsScreen" prefHeight="32.0"
		        prefWidth="40.0" styleClass="misc-menu-button">
			<graphic>
				<ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
					<Image url="@../icons/Settings.gif"/>
				</ImageView>
			</graphic>
		</Button>
	</HBox>
</fx:root>
